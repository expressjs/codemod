  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "29.5.14",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.8.1",
    "@types/prompts": "2.4.9",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18"
