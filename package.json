{
  "name": "@expressjs/codemod",
  "private": true,
  "version": "0.0.1",
  "description": "Codemods for updating express servers.",
  "contributors": ["Sebastian Beltran <bjohansebas@gmail.com>", "Filip Kudla <filip.kudla.dev@gmail.com>"],
  "license": "MIT",
  "bin": {
    "express-codemod": "build/index.js"
  },
  "files": ["build/**/*.js"],
  "scripts": {
